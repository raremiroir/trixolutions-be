		"svelte-turnstile": "^0.3.1",
		"tailwindcss": "^3.1.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.2",
		"vite-imagetools": "^4.0.12",
		"yup": "^1.0.0"
	},
	"keywords": [
		"trixolutions",
		"mist media",
		"coaching",
		"consultancy",
		"tom van dorst",
		"patrick lencioni",
		"lencioni"
	],
	"peerDependencies": {
		"leaflet": "^1.9.3"
	}
}
